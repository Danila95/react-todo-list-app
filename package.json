{
  "name": "react-redux-webpack-5-template",
  "version": "1.0.0",
  "description": "Self-made Webpack 5 template for web-projects where using: React, Redax, postcss",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open",
    "watch": "webpack --watch",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "dev": "cross-env NODE_ENV=development webpack --progress",
    "dash-start": "cross-env NODE_ENV=development webpack-dashboard -- webpack serve --mode development --progress --open",
    "prop": "webpack --env prop",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "run-db": "json-server db.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danila95/react-redux-webpack-5-template.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack 5",
    "postcss",
    "react",
    "redux"
  ],
  "author": "Bogdanov Danila <danila00000@Gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Danila95/react-redux-webpack-5-template/issues"
  },
  "homepage": "https://github.com/Danila95/react-redux-webpack-5-template#readme",
